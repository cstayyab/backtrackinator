{
    "manifest_version": 3,
    "name": "Backtrackinator",
    "description": "Never lose your place again! Backtrackinator remembers your last visited page on websites and takes you back with a single click",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "64": "icons/64.png",
        "128": "icons/128.png",
        "512": "icons/512.png"
      },
    "permissions": [
        "activeTab",
        "sessions",
        "tabs"
    ],
    "action": {
        "default_icon": {
            "16": "icons/16.png",
            "32": "icons/32.png",
            "64": "icons/64.png",
            "128": "icons/128.png",
            "512": "icons/512.png"
        },
        "default_title": "Backtrack this Website",
        "action": "resume"
    }
}